{
    "name": "package-start",
    "description": "Strate toolkit for creating PHP packages",
    "keywords": [
        "packages",
        "start"
    ],
    "homepage": "https://github.com/devbox12/package-start",
    "license": "MIT",
    "authors": [
        {
            "name": ":shawn sandy",
            "email": ":shawnsandy04@gmail.com",
            "homepage": ":shawnsandy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Package\\Start\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Package\\Start\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
